{
  "name": "plot-twist-fullstack",
  "version": "1.0.0",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd server && npm run dev",
    "client": "cd plot-twist-app && npm start",
    "install-all": "npm run install-server && npm run install-client",
    "install-server": "cd server && npm install",
    "install-client": "cd plot-twist-app && npm install"
  },
  "devDependencies": {
    "concurrently": "^8.2.0"
  },
  "dependencies": {
    "bcryptjs": "^3.0.3",
    "jsonwebtoken": "^9.0.2"
  }
}
